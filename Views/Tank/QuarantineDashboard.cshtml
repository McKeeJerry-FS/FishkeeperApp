@model AquaHub.MVC.Models.ViewModels.QuarantineDashboardViewModel
@{
    ViewData["Title"] = $"Quarantine Dashboard - {Model.Tank.Name}";
}

<div class="container-fluid">
    <!-- Header -->
    <div class="row mb-4">
        <div class="col">
            <h1 class="display-5">
                <i class="bi bi-shield-fill-plus text-warning"></i> Quarantine Dashboard
            </h1>
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a asp-action="Index">Tanks</a></li>
                    <li class="breadcrumb-item"><a asp-action="Details"
                                                       asp-route-id="@Model.Tank.Id">@Model.Tank.Name</a></li>
                        <li class="breadcrumb-item active">Quarantine</li>
                    </ol>
                </nav>
            </div>
        </div>

        <!-- Tank Info Card -->
        <div class="row mb-4">
            <div class="col">
                <div class="card border-warning">
                    <div class="card-header bg-warning text-dark">
                        <h5 class="card-title mb-0">
                            <i class="bi bi-info-circle"></i> @Model.Tank.Name - Quarantine Information
                        </h5>
                    </div>
                    <div class="card-body">
                        <div class="row">
                            <div class="col-md-6">
                                <p><strong>Tank Type:</strong> @Model.Tank.Type</p>
                                <p><strong>Capacity:</strong> @Model.Tank.VolumeGallons gallons</p>
                                <p><strong>Purpose:</strong> @Model.Tank.QuarantinePurpose</p>
                                <p><strong>Status:</strong> <span
                                    class="badge bg-@(Model.Tank.QuarantineStatus == "Active" ? "warning" : "secondary")">@Model.Tank.QuarantineStatus</span>
                                </p>
                            </div>
                            <div class="col-md-6">
                                <p><strong>Start Date:</strong> @Model.Tank.QuarantineStartDate?.ToString("MMM dd, yyyy")
                                </p>
                                <p><strong>Target End Date:</strong> @Model.Tank.QuarantineEndDate?.ToString("MMM dd, yyyy")
                                </p>
                                <p><strong>Days Elapsed:</strong> @Model.DaysInQuarantine days</p>
                                <p><strong>Days Remaining:</strong> @(Model.IsOverdue ? "OVERDUE" : $"{Model.RemainingDays} days")</p>
                            </div>
                        </div>

                        <!-- Progress Bar -->
                        <div class="mt-3">
                            <label class="form-label"><strong>Quarantine Progress:</strong></label>
                            <div class="progress" style="height: 30px;">
                                <div class="progress-bar @(Model.IsOverdue ? "bg-danger" : "bg-success")" role="progressbar"
                                     style="width: @Math.Min(Model.ProgressPercentage, 100)%"
                                     aria-valuenow="@Model.ProgressPercentage"                   aria-valuemin="0"                       aria-valuemax="100">
                                    @Model.ProgressPercentage.ToString("F0")%
                                </div>
                            </div>
                        </div>

                        @if (!string.IsNullOrEmpty(Model.Tank.TreatmentProtocol))
                        {
                            <div class="mt-3">
                                <p><strong>Treatment Protocol:</strong></p>
                                <div class="alert alert-info">@Model.Tank.TreatmentProtocol</div>
                            </div>
                        }
                    </div>
                </div>
            </div>
        </div>

        <!-- AI-Powered Care Recommendations -->
        @if (Model.AIRecommendations != null)
        {
            <div class="row mb-4">
                <div class="col">
                    <div class="card border-primary">
                        <div class="card-header bg-primary text-white">
                            <h5 class="card-title mb-0">
                                <i class="bi bi-robot"></i> AI-Powered Care Analysis & Recommendations
                            </h5>
                        </div>
                        <div class="card-body">
                            <!-- Overall Assessment -->
                            <div class="alert alert-@(Model.AIRecommendations.RiskLevel == "Critical" ? "danger" : 
                                                                                                               Model.AIRecommendations.RiskLevel == "High" ? "warning" :
                                                                                                               Model.AIRecommendations.RiskLevel == "Medium" ? "info" : "success")"
                                 role="alert">
                                <h5 class="alert-heading">
                                    @if (Model.AIRecommendations.RiskLevel == "Critical")
                                    {
                                        <i class="bi bi-exclamation-triangle-fill"></i>
                                    }
                                    else if (Model.AIRecommendations.RiskLevel == "High")
                                    {
                                        <i class="bi bi-exclamation-circle-fill"></i>
                                    }
                                    else if (Model.AIRecommendations.RiskLevel == "Medium")
                                    {
                                        <i class="bi bi-info-circle-fill"></i>
                                    }
                                    else
                                    {
                                        <i class="bi bi-check-circle-fill"></i>
                                    }
                                    Risk Level: @Model.AIRecommendations.RiskLevel
                                </h5>
                                <hr>
                                <p class="mb-0">@Html.Raw(Model.AIRecommendations.OverallAssessment.Replace("\n", "<br>"))</p>
                                @if (Model.AIRecommendations.RequiresImmediateAction)
                                {
                                    <div class="mt-2">
                                        <strong class="text-danger"><i class="bi bi-lightning-fill"></i> IMMEDIATE ACTION
                                            REQUIRED</strong>
                                        </div>
                                    }
                                </div>

                                <div class="row">
                                    <!-- Water Chemistry Insights -->
                                    @if (Model.AIRecommendations.WaterChemistryInsights.Any())
                                    {
                                        <div class="col-lg-6 mb-3">
                                            <div class="card h-100">
                                                <div class="card-header bg-light">
                                                    <h6 class="mb-0"><i class="bi bi-droplet"></i> Water Chemistry Analysis</h6>
                                                </div>
                                                <div class="card-body">
                                                    <ul class="list-unstyled mb-0">
                                                        @foreach (var insight in Model.AIRecommendations.WaterChemistryInsights)
                                                        {
                                                            <li class="mb-2">
                                                                <small>@Html.Raw(insight.Replace("\n", "<br>"))</small>
                                                            </li>
                                                        }
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    }

                                    <!-- Dosing Recommendations -->
                                    @if (Model.AIRecommendations.DosingRecommendations.Any())
                                    {
                                        <div class="col-lg-6 mb-3">
                                            <div class="card h-100">
                                                <div class="card-header bg-light">
                                                    <h6 class="mb-0"><i class="bi bi-capsule"></i> Treatment & Dosing Guidance</h6>
                                                </div>
                                                <div class="card-body">
                                                    <ul class="list-unstyled mb-0">
                                                        @foreach (var rec in Model.AIRecommendations.DosingRecommendations)
                                                        {
                                                            <li class="mb-2">
                                                                <small>@Html.Raw(rec.Replace("\n", "<br>"))</small>
                                                            </li>
                                                        }
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    }

                                    <!-- Maintenance Actions -->
                                    @if (Model.AIRecommendations.MaintenanceActions.Any())
                                    {
                                        <div class="col-lg-6 mb-3">
                                            <div class="card h-100">
                                                <div class="card-header bg-light">
                                                    <h6 class="mb-0"><i class="bi bi-wrench"></i> Maintenance Recommendations</h6>
                                                </div>
                                                <div class="card-body">
                                                    <ul class="list-unstyled mb-0">
                                                        @foreach (var action in Model.AIRecommendations.MaintenanceActions)
                                                        {
                                                            <li class="mb-2">
                                                                <small>@Html.Raw(action.Replace("\n", "<br>"))</small>
                                                            </li>
                                                        }
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    }

                                    <!-- Monitoring Priorities -->
                                    @if (Model.AIRecommendations.MonitoringPriorities.Any())
                                    {
                                        <div class="col-lg-6 mb-3">
                                            <div class="card h-100">
                                                <div class="card-header bg-light">
                                                    <h6 class="mb-0"><i class="bi bi-eye"></i> Monitoring Priorities</h6>
                                                </div>
                                                <div class="card-body">
                                                    <ul class="list-unstyled mb-0">
                                                        @foreach (var priority in Model.AIRecommendations.MonitoringPriorities)
                                                        {
                                                            <li class="mb-2">
                                                                <small>@Html.Raw(priority.Replace("\n", "<br>"))</small>
                                                            </li>
                                                        }
                                                    </ul>
                                                </div>
                                            </div>
                                        </div>
                                    }
                                </div>

                                <!-- Next Steps -->
                                @if (Model.AIRecommendations.NextSteps.Any())
                                {
                                    <div class="card mt-3">
                                        <div class="card-header bg-success text-white">
                                            <h6 class="mb-0"><i class="bi bi-list-check"></i> Recommended Next Steps</h6>
                                        </div>
                                        <div class="card-body">
                                            <ol class="mb-0">
                                                @foreach (var step in Model.AIRecommendations.NextSteps)
                                                {
                                                    <li class="mb-2">@Html.Raw(step.Replace("\n", "<br>"))</li>
                                                }
                                            </ol>
                                        </div>
                                    </div>
                                }

                                <!-- Treatment Guidance -->
                                @if (Model.AIRecommendations.TreatmentGuidance.Any())
                                {
                                    <div class="card mt-3">
                                        <div class="card-header bg-info text-white">
                                            <h6 class="mb-0"><i class="bi bi-clipboard2-pulse"></i> Treatment Protocol Guidance</h6>
                                        </div>
                                        <div class="card-body">
                                            <div class="row">
                                                @foreach (var guidance in Model.AIRecommendations.TreatmentGuidance)
                                                {
                                                    <div class="col-md-6 mb-2">
                                                        <strong>@guidance.Key:</strong> @guidance.Value
                                                    </div>
                                                }
                                            </div>
                                        </div>
                                    </div>
                                }

                                <div class="text-muted text-end mt-3">
                                    <small><i class="bi bi-clock"></i> Analysis generated:
                                        @Model.AIRecommendations.GeneratedAt.ToString("MMM dd, yyyy h:mm tt")</small>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                }

                <!-- Alerts Row -->
                @if (Model.WaterQualityAlerts.Any() || Model.NeedsWaterTest || Model.NeedsWaterChange)
                {
                    <div class="row mb-4">
                        <div class="col">
                            <div class="card border-danger">
                                <div class="card-header bg-danger text-white">
                                    <h5 class="card-title mb-0"><i class="bi bi-exclamation-triangle"></i> Alerts & Warnings</h5>
                                </div>
                                <div class="card-body">
                                    @foreach (var alert in Model.WaterQualityAlerts)
                                    {
                                        <div class="alert alert-warning mb-2">@alert</div>
                                    }
                                    @if (Model.NeedsWaterTest)
                                    {
                                        <div class="alert alert-warning mb-2">
                                            <i class="bi bi-beaker"></i> Water testing is overdue. Last test was over 3 days ago.
                                        </div>
                                    }
                                    @if (Model.NeedsWaterChange)
                                    {
                                        <div class="alert alert-warning mb-2">
                                            <i class="bi bi-arrow-repeat"></i> Water change is overdue. Last change was
                                            @Model.DaysSinceWaterChange days ago.
                                        </div>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>
                }

                <!-- Quick Actions -->
                <div class="row mb-4">
                    <div class="col">
                        <div class="card">
                            <div class="card-header">
                                <h5 class="card-title mb-0"><i class="bi bi-lightning"></i> Quick Actions</h5>
                            </div>
                            <div class="card-body">
                                <div class="btn-group" role="group">
                                    <a asp-controller="WaterTest" asp-action="Create" asp-route-tankId="@Model.Tank.Id"
                                           class="btn btn-primary">
                                        <i class="bi bi-droplet-fill"></i> Record Water Test
                                    </a>
                                    <a asp-controller="DosingRecord" asp-action="Create" asp-route-tankId="@Model.Tank.Id"
                                           class="btn btn-info">
                                        <i class="bi bi-capsule"></i> Log Treatment/Dose
                                    </a>
                                    <a asp-controller="Feeding" asp-action="Create" asp-route-tankId="@Model.Tank.Id"
                                           class="btn btn-success">
                                        <i class="bi bi-egg"></i> Record Feeding
                                    </a>
                                    <a asp-controller="MaintenanceLog" asp-action="Create" asp-route-tankId="@Model.Tank.Id"
                                           class="btn btn-warning">
                                        <i class="bi bi-wrench"></i> Log Maintenance
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Current Water Parameters -->
                <div class="row mb-4">
                    <div class="col">
                        <div class="card">
                            <div class="card-header">
                                <h5 class="card-title mb-0"><i class="bi bi-thermometer-half"></i> Current Water Parameters</h5>
                            </div>
                            <div class="card-body">
                                @if (Model.LatestWaterTest != null)
                                {
                                    <div class="row text-center">
                                        <div class="col-md-2 col-sm-4 mb-3">
                                            <div class="card @(Model.LatestWaterTest.PH < 7.8 || Model.LatestWaterTest.PH > 8.4 ? "border-warning" : "")">
                                                <div class="card-body">
                                                    <h6>pH</h6>
                                                    <h4>@Model.LatestWaterTest.PH?.ToString("F2")</h4>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2 col-sm-4 mb-3">
                                            <div class="card @(Model.LatestWaterTest.Temperature < 74 || Model.LatestWaterTest.Temperature > 80 ? "border-warning" : "")">
                                                <div class="card-body">
                                                    <h6>Temp</h6>
                                                    <h4>@Model.LatestWaterTest.Temperature?.ToString("F1")°F</h4>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2 col-sm-4 mb-3">
                                            <div class="card @(Model.LatestWaterTest.Ammonia > 0 ? "border-danger" : "")">
                                                <div class="card-body">
                                                    <h6>Ammonia</h6>
                                                    <h4>@Model.LatestWaterTest.Ammonia?.ToString("F2")</h4>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2 col-sm-4 mb-3">
                                            <div class="card @(Model.LatestWaterTest.Nitrite > 0 ? "border-danger" : "")">
                                                <div class="card-body">
                                                    <h6>Nitrite</h6>
                                                    <h4>@Model.LatestWaterTest.Nitrite?.ToString("F2")</h4>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2 col-sm-4 mb-3">
                                            <div class="card @(Model.LatestWaterTest.Nitrate > 40 ? "border-warning" : "")">
                                                <div class="card-body">
                                                    <h6>Nitrate</h6>
                                                    <h4>@Model.LatestWaterTest.Nitrate?.ToString("F1")</h4>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-2 col-sm-4 mb-3">
                                            <div class="card">
                                                <div class="card-body">
                                                    <h6>Salinity</h6>
                                                    <h4>@Model.LatestWaterTest.Salinity?.ToString("F3")</h4>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <p class="text-muted text-end mb-0">
                                        <small>Last tested: @Model.LatestWaterTest.Timestamp.ToString("MMM dd, yyyy h:mm tt")</small>
                                    </p>
                                }
                                else
                                {
                                    <div class="alert alert-info">
                                        No water test data available. <a asp-controller="WaterTest" asp-action="Create"
                                                                         asp-route-tankId="@Model.Tank.Id">Record a water test</a>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Water Parameter Trends Chart -->
                @if (Model.RecentWaterTests.Any())
                {
                    <div class="row mb-4">
                        <div class="col">
                            <div class="card">
                                <div class="card-header">
                                    <h5 class="card-title mb-0"><i class="bi bi-graph-up"></i> Water Parameter Trends (Last 14 Days)
                                    </h5>
                                </div>
                                <div class="card-body">
                                    <canvas id="waterParametersChart" style="max-height: 400px;"></canvas>
                                </div>
                            </div>
                        </div>
                    </div>
                }

                <!-- Quarantined Livestock -->
                <div class="row mb-4">
                    <div class="col">
                        <div class="card">
                            <div class="card-header">
                                <h5 class="card-title mb-0"><i class="bi bi-fish"></i> Quarantined Livestock
                                    (@Model.QuarantinedLivestock.Count)</h5>
                                </div>
                                <div class="card-body">
                                    @if (Model.QuarantinedLivestock.Any())
                                    {
                                        <div class="row">
                                            @foreach (var livestock in Model.QuarantinedLivestock)
                                            {
                                                <div class="col-md-4 mb-3">
                                                    <div class="card">
                                                        <div class="card-body">
                                                            <h6>@livestock.Species</h6>
                                                            @if (!string.IsNullOrEmpty(livestock.Name))
                                                            {
                                                                <p class="text-muted mb-1">@livestock.Name</p>
                                                            }
                                                            <p class="mb-1"><small><strong>Added:</strong> @livestock.AddedOn.ToString("MMM dd, yyyy")</small></p>
                                                            @if (!string.IsNullOrEmpty(livestock.Notes))
                                                            {
                                                                <p class="mb-0"><small><strong>Notes:</strong> @livestock.Notes</small></p>
                                                            }
                                                        </div>
                                                    </div>
                                                </div>
                                            }
                                        </div>
                                    }
                                    else
                                    {
                                        <p class="text-muted">No livestock currently in quarantine.</p>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Two Column Layout: Treatments/Dosing & Feeding -->
                    <div class="row mb-4">
                        <!-- Active Treatments & Dosing -->
                        <div class="col-lg-6 mb-3">
                            <div class="card h-100">
                                <div class="card-header">
                                    <h5 class="card-title mb-0"><i class="bi bi-prescription2"></i> Active Treatments & Dosing</h5>
                                </div>
                                <div class="card-body">
                                    @if (Model.ActiveTreatments.Any())
                                    {
                                        <div class="table-responsive">
                                            <table class="table table-sm table-hover">
                                                <thead>
                                                    <tr>
                                                        <th>Date</th>
                                                        <th>Chemical</th>
                                                        <th>Amount</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var dose in Model.ActiveTreatments.OrderByDescending(d =>
                                                                                    d.Timestamp).Take(5))
                                                    {
                                                        <tr>
                                                            <td>@dose.Timestamp.ToString("MM/dd")</td>
                                                            <td>@dose.Additive</td>
                                                            <td>@dose.AmountMl mL</td>
                                                        </tr>
                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                    }
                                    else
                                    {
                                        <p class="text-muted">No active treatments.</p>
                                    }
                                </div>
                            </div>
                        </div>

                        <!-- Feeding Schedule & Recent Feedings -->
                        <div class="col-lg-6 mb-3">
                            <div class="card h-100">
                                <div class="card-header">
                                    <h5 class="card-title mb-0"><i class="bi bi-egg-fried"></i> Feeding Schedule & Recent Feedings</h5>
                                </div>
                                <div class="card-body">
                                    @if (Model.RecentFeedings.Any())
                                    {
                                        <div class="table-responsive">
                                            <table class="table table-sm table-hover">
                                                <thead>
                                                    <tr>
                                                        <th>Date</th>
                                                        <th>Food Type</th>
                                                        <th>Amount</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var feeding in Model.RecentFeedings.Take(5))
                                                    {
                                                        <tr>
                                                            <td>@feeding.FedDateTime.ToString("MM/dd HH:mm")</td>
                                                            <td>@feeding.FeedName</td>
                                                            <td>@feeding.Amount @feeding.Unit</td>
                                                        </tr>
                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                    }
                                    else
                                    {
                                        <p class="text-muted">No feeding records.</p>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Recent Maintenance -->
                    <div class="row mb-4">
                        <div class="col">
                            <div class="card">
                                <div class="card-header">
                                    <h5 class="card-title mb-0"><i class="bi bi-tools"></i> Recent Maintenance</h5>
                                </div>
                                <div class="card-body">
                                    @if (Model.RecentMaintenance.Any())
                                    {
                                        <div class="table-responsive">
                                            <table class="table table-hover">
                                                <thead>
                                                    <tr>
                                                        <th>Date</th>
                                                        <th>Type</th>
                                                        <th>Notes</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    @foreach (var log in Model.RecentMaintenance)
                                                    {
                                                        <tr>
                                                            <td>@log.Timestamp.ToString("MMM dd, yyyy")</td>
                                                            <td><span class="badge bg-secondary">@log.Type</span></td>
                                                            <td>@log.Notes</td>
                                                        </tr>
                                                    }
                                                </tbody>
                                            </table>
                                        </div>
                                    }
                                    else
                                    {
                                        <p class="text-muted">No maintenance records.</p>
                                    }
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Back Button -->
                    <div class="row">
                        <div class="col">
                            <a asp-action="Details" asp-route-id="@Model.Tank.Id" class="btn btn-secondary">
                                <i class="bi bi-arrow-left"></i> Back to Tank Details
                            </a>
                        </div>
                    </div>
                </div>

                @section Scripts {
                    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.4.0/dist/chart.umd.min.js"></script>
                    <script>
                        // Water Parameters Trend Chart
                        @if (Model.RecentWaterTests.Any())
                        {
                            <text>
                                const ctx = document.getElementById('waterParametersChart');
                                new Chart(ctx, {
                                type: 'line',
                                data: {
                                labels: @Html.Raw(Json.Serialize(Model.ChartLabels)),
                                datasets: [
                                {
                                label: 'pH',
                                data: @Html.Raw(Json.Serialize(Model.PHData)),
                                borderColor: 'rgb(75, 192, 192)',
                                backgroundColor: 'rgba(75, 192, 192, 0.1)',
                                tension: 0.1,
                                yAxisID: 'y'
                                },
                                {
                                label: 'Temperature (°F)',
                                data: @Html.Raw(Json.Serialize(Model.TemperatureData)),
                                borderColor: 'rgb(255, 99, 132)',
                                backgroundColor: 'rgba(255, 99, 132, 0.1)',
                                tension: 0.1,
                                yAxisID: 'y1'
                                },
                                {
                                label: 'Ammonia (ppm)',
                                data: @Html.Raw(Json.Serialize(Model.AmmoniaData)),
                                borderColor: 'rgb(255, 205, 86)',
                                backgroundColor: 'rgba(255, 205, 86, 0.1)',
                                tension: 0.1,
                                yAxisID: 'y2'
                                },
                                {
                                label: 'Nitrite (ppm)',
                                data: @Html.Raw(Json.Serialize(Model.NitriteData)),
                                borderColor: 'rgb(153, 102, 255)',
                                backgroundColor: 'rgba(153, 102, 255, 0.1)',
                                tension: 0.1,
                                yAxisID: 'y2'
                                },
                                {
                                label: 'Nitrate (ppm)',
                                data: @Html.Raw(Json.Serialize(Model.NitrateData)),
                                borderColor: 'rgb(54, 162, 235)',
                                backgroundColor: 'rgba(54, 162, 235, 0.1)',
                                tension: 0.1,
                                yAxisID: 'y2'
                                }
                                ]
                                },
                                options: {
                                responsive: true,
                                maintainAspectRatio: false,
                                interaction: {
                                mode: 'index',
                                intersect: false
                                },
                                plugins: {
                                legend: {
                                position: 'top',
                                },
                                title: {
                                display: false
                                }
                                },
                                scales: {
